{
    "name": "d3cr33/online-manage-routes",
    "description": "Manage Laravel Routes",
    "keywords": [
        "d3cr33",
        "laravel",
        "routes"
    ],
    "homepage": "https://github.com/HosseinAlikhani/online-manage-routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Hossein Alikhani",
            "email": "hossein.alikhani1374@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/auth": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6",
        "fakerphp/faker": "^1.20",
        "phpunit/phpunit": "^9.5.10",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "D3cr33\\Routes\\": "src",
            "D3cr33\\Routes\\Test\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D3cr33\\Routes\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "D3cr33\\Routes\\RouteServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev",
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
