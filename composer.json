{
    "name": "d3cr33/manage-routes",
    "description": "Manage Laravel Routes",
    "keywords": [
        "d3cr33",
        "laravel",
        "routes"
    ],
    "homepage": "https://github.com/HosseinAlikhani/d3cr33-manage-routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Hossein Alikhani",
            "email": "hossein.alikhani1374@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2.5|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "D3cr33\\Routes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "D3cr33\\Routes\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "D3cr33\\Routes\\RouteServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.x-dev",
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}